[vpn]
vpn ansible_host=1.2.3.4 ansible_user=root ansible_ssh_private_key_file=/path/to/private/key

[backend]
backend ansible_host=1.2.3.4 ansible_user=root ansible_ssh_private_key_file=/path/to/private/key

[frontend]
frontend ansible_host=1.2.3.4 ansible_user=root ansible_ssh_private_key_file=/path/to/private/key

[worker]
worker1 ansible_host=1.2.3.4 ansible_user=root ansible_ssh_private_key_file=/path/to/private/key
worker2 ansible_host=1.2.3.4 ansible_user=root ansible_ssh_private_key_file=/path/to/private/key